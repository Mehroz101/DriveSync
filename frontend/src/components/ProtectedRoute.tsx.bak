import { Navigate } from "react-router-dom";
import { getUserId } from "../utils/auth";
import { getAuthToken } from "../api/auth.api";
import type { JSX } from "react";

const ProtectedRoute = ({ children }: { children: JSX.Element }) => {
  // Check if user is authenticated via Google OAuth or JWT
  const hasGoogleAuth = getUserId();
  const hasJWTAuth = !!getAuthToken();
  
  return (hasGoogleAuth || hasJWTAuth) ? children : <Navigate to="/" replace />;
};

export default ProtectedRoute;
  
